{
  "name": "calendrax",
  "private": false,
  "version": "0.6.1",
  "type": "module",
  "description": "A powerful React/TypeScript calendar component with advanced booking features, minimum nights, day info display, and customizable date range selection",
  "author": "Bidipto Bose <bdbose123@gmail.com> (https://bdbose.in)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bdbose/calendrax"
  },
  "keywords": [
    "react",
    "calendar",
    "date-picker",
    "date-range",
    "typescript",
    "events",
    "booking",
    "hotel",
    "vacation-rental",
    "minimum-nights",
    "blocked-dates",
    "day-info",
    "price-display",
    "responsive",
    "customizable"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "setup:example": "bash scripts/setup-example.sh",
    "prepare:publish": "bash scripts/prepare-publish.sh"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "rollup": "^4.52.4",
    "rollup-plugin-import-css": "^3.5.8",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
}
